Before (use test driver):
  let g:zcd#driver = 'test'

Execute (returns the results from the corresponding driver):
  let result1 = { 'frecency': 1, 'directory': '/dir1' }
  let result2 = { 'frecency': 2, 'directory': '/dir2' }
  let zcd#test_driver#results = [result1, result2]
  AssertEqual zcd#test_driver#results, zcd#FindMatches('query')

Execute (expands special symbols):
  let $Z_ORIGIN = getcwd()

  Z ~
  AssertEqual $HOME . '/', expand('%:p')

  Z $Z_ORIGIN
  AssertEqual $Z_ORIGIN . '/', expand('%:p')
