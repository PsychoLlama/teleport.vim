Before (reset config):
  unlet! zcd#path
  unlet! zcd#driver

Execute (loads the correct driver):
  let zcd#driver = 'zoxide'
  AssertEqual zcd#drivers#zoxide#(), zcd#driver#load()

  let zcd#driver = 'z'
  AssertEqual zcd#drivers#rupaz#(), zcd#driver#load()

  let zcd#driver = 'z.lua'
  AssertEqual zcd#drivers#zlua#(), zcd#driver#load()

  let zcd#driver = 'test'
  AssertEqual zcd#drivers#test#(), zcd#driver#load()

  let zcd#driver = 'invalid'
  silent AssertEqual v:null, zcd#driver#load()

Execute (returns null if the path is invalid):
  let zcd#path = 'invalid'
  let zcd#driver = 'test'

  silent AssertEqual v:null, zcd#driver#load()
